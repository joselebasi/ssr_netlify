{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "src/components/*"
      ],
      "@layouts/*": [
        "src/layouts/*"
      ],
      "@pages/*": [
        "src/pages/*"
      ],
      "@lib/*": [
        "sc/lib/*"
      ]
    },

  },
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"]
}